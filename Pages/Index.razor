@page "/"


<CascadingValue Value="this">
    <ProfileEditor /> 
    <ProfileViewer />
</CascadingValue>

@code {
    public string Name {get; private set;} = "";
    public int Age {get; private set;}

    public void SetName(string value){
        Name = value;
        StateHasChanged();
    }

    public void SetAge(int value) {
        Age = value;
        StateHasChanged();
    }
}

